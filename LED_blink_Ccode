#include<lpc17xx.h> //it is common for the arm proccessor FIO- fast
void delay(unsigned int a);
int main()
{
LPC_GPIO1->FIODIR =(1<<19); //(0XFF<<19)
while(1)
{
LPC_GPIO1->FIOSET=(1<<19);
delay(500);
LPC_GPIO1->FIOCLR=(1<<19);
delay(500);
}
}
void delay(unsigned int a)
{
 unsigned int i,j;
for(i=0;i<a;i++)
for(j=0;j<5000;j++); //5000*500/64
}
